		"@aws-cdk/aws-cloudfront": "^1.64.0",
		"@aws-cdk/aws-cognito": "^1.64.0",
		"@aws-cdk/aws-dynamodb": "^1.64.0",
		"@aws-cdk/aws-ec2": "^1.64.0",
		"@aws-cdk/aws-ecs": "^1.64.0",
		"@aws-cdk/aws-iam": "^1.64.0",
		"@aws-cdk/aws-lambda": "^1.64.0",

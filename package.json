{
	"name": "drawspace-be",
	"version": "0.1.0",
	"private": "true",
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"us": "NODE_PATH=./src ./node_modules/.bin/jest --updateSnapshot",
		"test": "NODE_PATH=./src ./node_modules/.bin/jest",
		"cdk": "cdk",
		"webpackLambdas": "node ./src/lambdas/util/webpackLambdas.js",
		"deploy:layers": "./node_modules/.bin/cdk deploy DrawSpaceLambdaLayersStaging --profile DrawSpaceStagingAdmin",
		"deploy:cognito": "yarn webpackLambdas && ./node_modules/.bin/cdk deploy DrawSpaceCognitoStaging --profile DrawSpaceStagingAdmin",
		"deploy:uploads": "./node_modules/.bin/cdk deploy DrawSpaceUploadsStaging --profile DrawSpaceStagingAdmin",
		"deploy:mediaEncode": "yarn webpackLambdas && ./node_modules/.bin/cdk deploy DrawSpaceMediaEncodeStaging --profile DrawSpaceStagingAdmin",
		"deploy:restApi": "yarn webpackLambdas && ./node_modules/.bin/cdk deploy DrawSpaceRestApiStaging --profile DrawSpaceStagingAdmin",
		"deploy:ddb": "./node_modules/.bin/cdk deploy DrawSpaceApiDbStaging --profile DrawSpaceStagingAdmin",
		"deploy:webHost": "./node_modules/.bin/cdk deploy DrawSpaceWebHostStaging --profile DrawSpaceStagingAdmin"
	},
	"devDependencies": {
		"@aws-cdk/assert": "1.46.0",
